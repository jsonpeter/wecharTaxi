.map_container{height: 53.5%;display:block; }
.map_container map{height: 100%; width: 100%; }
.disabled{ background: #CCCCCC;}
.block{ width: 100%; display: block;}
.beizu{ font-size: 13px; height: 40px; margin-top: 2px; text-align: center;}
.beizu text{display: block; width: 100%; line-height: 22px;color: #666;}
image {
    width: 150rpx;
    height: 150rpx;
}
.page-body-time {
    display: flex;
    flex-direction: column;
    align-items: center;
}
.time-big {
    font-size: 60rpx;
    margin: 20rpx;
}
.time-small {
    font-size: 30rpx;
}

.page-body-buttons {
    margin-top: 10rpx;
    display: flex;
    justify-content: space-around;
}
.page-body-button {
    width: 250rpx;
    text-align: center;
}
.button-stop-record {
    width: 110rpx;
    height: 110rpx;
    border: 20rpx solid #fff;
    background-color: #f55c23;
    border-radius: 130rpx;
    margin: 0 auto;
}
