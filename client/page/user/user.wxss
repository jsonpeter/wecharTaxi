/*.page-mobile{ }*/
.btn-code{ width: 118px;}
.userimg{ text-align: center; margin:20px 0; }
.userimg image{ width: 80px; height: 80px; border-radius: 100%; border: 1px solid #FDFDFD;}
.userimg text{display: block;color: #FF9900;font-size: 14px;font-weight: bold;}
.order-list{ height: 70%; text-align: center;}
.order-list view.list{width:calc(100% - 30px); padding: 5px; margin: 10px;background: #FFFFFF; border: 1px solid #d9d9d9;border-radius: 5px;}
.order-list view.list image{ width: 64px; height: 64px; border-radius: 100%; border: 1px solid #CCCCCC;}
.order-list .list-b-top text{ height: 30px; line-height: 30px;}
.order-list .list-b-top{height: 30px; display: block;}
.order-list .list-b-top .datetime{font-weight:bolder;color:#FF9900; float: right; width: 80px; text-align: center;}
.order-list text.gender{padding-left: 35px; float: left;}
.con view .dian::before{top:2px;left: 0;}
.m-top{ margin-bottom: 5px;}
.price{display: block; height: 35px; width: 100%;}
.con{width: calc(100% - 100px)}
.con view{text-align: left;color: #666666; }
.order-list .con{position: relative;}
.order-list .con .money{color: #FF9900; font-weight: bolder; width: 80px; font-size: 16px;position: absolute; right: 0; top: 30px;}
.order-list .con .money .block{color: #999;font-weight: normal; font-size: 12px; display: block; height:16px; line-height:16px;}
.userimg image{ vertical-align: middle;}
.order-list .list-bot{ height:40px; line-height: 40px;border-top:1px dashed #d9d9d9; text-align: left;}
.order-list .list-bot text,.order-list .list-bot image{  vertical-align: middle; display: inline-block;  line-height: 30px; }

.order-list .list-bot .voice image{
    border-radius: inherit;
    width: 120px;height: 30px;
    border: none;
}
.address view{white-space:nowrap;text-overflow:ellipsis;overflow:hidden;-webkit-text-overflow:ellipsis; width: 100%; margin-top: 5px;display: block;}
.order-list .list-bot .btn-order{width: 80px;border: 1px solid #FF9900; color: #FF9900; text-align: center;border-radius: 5px; }

.listbox{ margin-bottom: 5px;}

.order-list text.gender.g-1{background: url("data:image/jpeg;base64,") left no-repeat;}
.order-list view text.gender.g-2{background: url("data:image/jpeg;base64,") left no-repeat;}
